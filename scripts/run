#   -*-  grammar-ext: sh  -*-
 
set -e
cd ~/dev/apps/jsw
scripts/compile 
export DEBUG=*
if [[ $1 == genonly ]]; then 
  echo "running gen only"
  node lib/jsw.js -g  js
else if [[ $1 == parseonly ]]; then 
    echo "running parse only"
    node lib/jsw.js -p  js
  else 
    node lib/jsw.js -pg js
  fi
fi
exit 0  # $?
